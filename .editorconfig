root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{rs}]
indent_style = space
indent_size = 4


[*.{c,h,cpp,hpp,rs}]
indent_style = tab
indent_size = 4

[makefile]
indent_style = tab
indent_size = 1

[*.{yaml,json}]
indent_style = space
indent_size = 2
